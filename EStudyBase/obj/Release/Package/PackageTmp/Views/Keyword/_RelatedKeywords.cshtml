@model EStudyBase.ViewModels.SearchViewModel

<table class="table table-striped table-bordered table-condensed table-hover table-centered" style="margin-top:25px;">
    <tr>
        <td colspan="5 ">
            <div class="content-header">
                <h5>Aradığınız başlığa en yakın başlıklar... <span class="pull-right"><i class="icon-th"></i></span></h5>
            </div>
        </td>
    </tr>
    <tr>
    @foreach (var keywordViewModel in Model.RelatedKeywords)
    {
        
            <td>
                <a href="/Keyword/Search?keywordId=@keywordViewModel.KeywordId" data-toggle="tooltip" 
                   title="@keywordViewModel.Definition">@keywordViewModel.Definition</a>
            </td>
        
    }
        </tr>
</table>
